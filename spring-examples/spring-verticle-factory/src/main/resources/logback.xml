<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%level [%date{yyyy-MM-dd HH:mm:ss.sss}] [%thread] %logger{0} - %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="io.netty" level="warn"/>
  <root level="debug">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>
